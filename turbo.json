{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "@scriptum/desktop#build": {
      "outputs": []
    },
    "@scriptum/editor#build": {
      "outputs": []
    },
    "@scriptum/shared#build": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": ["coverage/**"]
    },
    "lint": {
      "outputs": []
    }
  }
}
